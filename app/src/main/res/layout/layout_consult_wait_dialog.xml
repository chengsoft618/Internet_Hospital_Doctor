<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="35dp"
        android:background="@drawable/bg_solid_ffffff_radius_10"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            android:id="@+id/layout_consult_wait_title_tv"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:gravity="center"
            android:text="@string/consult_wait_title_applying"
            android:textColor="@color/color_app_main_color"
            android:textSize="@dimen/font_size_20" />

        <View
            style="@style/horizontal_line_light_1"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="@string/video_room_scheduling_time"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/font_size_18" />

        <TextView
            android:id="@+id/layout_consult_wait_date_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/font_size_16" />

        <TextView
            android:id="@+id/layout_consult_wait_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/font_size_16" />

        <TextView
            android:id="@+id/layout_consult_wait_sequence_num_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/font_size_16" />

        <TextView
            android:id="@+id/layout_consult_wait_predict_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/font_size_16" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="30dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/layout_consult_wait_superior_doctor_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/ic_big_video_super_expert"
                android:drawableLeft="@drawable/ic_big_video_super_expert"
                android:drawablePadding="10dp"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:lines="1"
                android:paddingLeft="25dp"
                android:paddingRight="25dp"
                android:textColor="@color/color_666666"
                android:textSize="@dimen/font_size_16" />

            <TextView
                android:id="@+id/layout_consult_wait_attending_doctor_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:drawableStart="@drawable/ic_big_video_first_expert"
                android:drawableLeft="@drawable/ic_big_video_first_expert"
                android:drawablePadding="10dp"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:lines="1"
                android:paddingLeft="25dp"
                android:paddingRight="25dp"
                android:textColor="@color/color_666666"
                android:textSize="@dimen/font_size_16" />

            <TextView
                android:id="@+id/layout_consult_wait_patient_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:drawableStart="@drawable/ic_big_video_patient_icon"
                android:drawableLeft="@drawable/ic_big_video_patient_icon"
                android:drawablePadding="10dp"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:lines="1"
                android:paddingLeft="25dp"
                android:paddingRight="25dp"
                android:textColor="@color/color_666666"
                android:textSize="@dimen/font_size_16" />
        </LinearLayout>

        <TextView
            android:id="@+id/layout_consult_wait_confirm_tv"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:background="@drawable/bg_dialog_btn_bottom_45aef8_corners"
            android:gravity="center"
            android:text="@string/sure"
            android:textColor="@color/color_white"
            android:textSize="@dimen/font_size_18" />
    </LinearLayout>
</LinearLayout>
