<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:gravity="center_vertical">

    <LinearLayout
        android:id="@+id/item_dc_duty_data_up_load_voice_ll"
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:layout_below="@+id/item_dc_duty_data_up_load_voice_del_iv"
        android:layout_marginRight="10dp"
        android:background="@drawable/bg_solid_82cfff_radius_2"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/item_dc_duty_data_up_load_voice_iv"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginLeft="10dp"
            android:src="@mipmap/ic_dc_duty_patient_voice" />

        <TextView
            android:id="@+id/item_dc_duty_data_up_load_voice_time_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:gravity="end"
            android:textColor="@color/color_0f6499"
            android:textSize="@dimen/font_size_14" />
    </LinearLayout>

    <ImageView
        android:id="@+id/item_dc_duty_data_up_load_voice_del_iv"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginLeft="-20dp"
        android:layout_marginBottom="-10dp"
        android:layout_toRightOf="@+id/item_dc_duty_data_up_load_voice_ll"
        android:src="@mipmap/ic_doctor_list_search_clear" />

</RelativeLayout>