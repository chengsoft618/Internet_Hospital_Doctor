<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/item_dc_duty_data_up_load_pic_iv"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_below="@+id/item_dc_duty_data_up_load_pic_del_iv"
        android:scaleType="fitXY" />

    <ImageView
        android:id="@+id/item_dc_duty_data_up_load_pic_del_iv"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginLeft="-10dp"
        android:layout_marginBottom="-10dp"
        android:layout_toRightOf="@+id/item_dc_duty_data_up_load_pic_iv"
        android:src="@mipmap/ic_doctor_list_search_clear" />
</RelativeLayout>