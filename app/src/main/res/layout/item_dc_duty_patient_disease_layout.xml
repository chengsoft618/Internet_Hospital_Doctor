<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_white">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/item_dc_duty_patient_disease_time_line_view"
            android:layout_width="1dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22dp"
            android:background="@color/color_e6e6e6"
            android:visibility="visible" />

        <LinearLayout
            android:id="@+id/item_dc_duty_patient_disease_top_content_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:gravity="center_vertical">

            <ImageView
                android:id="@+id/item_dc_duty_patient_disease_top_diagnostic_circular"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/ic_dc_duty_patient_disease_top_diagnostic_circular" />

            <TextView
                android:id="@+id/item_dc_duty_patient_disease_treat_type_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="复诊"
                android:textColor="@color/color_049eff"
                android:textSize="@dimen/font_size_16" />

            <TextView
                android:id="@+id/item_dc_duty_patient_disease_treat_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:gravity="center"
                android:text="2019.10.23"
                android:textColor="#ff999999"
                android:textSize="@dimen/font_size_14" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/item_dc_duty_patient_disease_bottom_content_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_dc_duty_patient_disease_top_content_ll"
            android:layout_marginLeft="@dimen/dp_40"
            android:background="@color/white"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/item_dc_duty_patient_disease_diagnosis_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:gravity="center_vertical">

                <View
                    android:id="@+id/item_dc_duty_patient_disease_diagnosis_v"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_gravity="center"
                    android:background="@drawable/bg_solid_666666_patient_point" />

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_diagnosis_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/item_dc_duty_patient_disease_diagnosis_v"
                    android:gravity="center|left"
                    android:text="疾病诊断:"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/font_size_14" />

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_diagnosis_desc_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="16dp"
                    android:layout_toRightOf="@+id/item_dc_duty_patient_disease_diagnosis_tv"
                    android:layout_weight="1"
                    android:gravity="center|left"
                    android:singleLine="true"
                    android:text="慢性阻塞性肺疾病反反复复发烧佛哦吼粉红佛 off 放开那就是对方 i 哦肌肤防腐剂哦素"
                    android:textColor="@color/color_1a1a1a"
                    android:textSize="@dimen/font_size_14"
                    android:visibility="visible" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/item_dc_duty_patient_disease_survey_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical">

                <View
                    android:id="@+id/item_dc_duty_patient_disease_survey_v"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_below="@+id/item_dc_duty_patient_disease_diagnosis_tv"
                    android:layout_gravity="center"
                    android:background="@drawable/bg_solid_666666_patient_point" />

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_survey_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/item_dc_duty_patient_disease_diagnosis_tv"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/item_dc_duty_patient_disease_survey_v"
                    android:gravity="center|left"
                    android:text="病情摘要:"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/font_size_14" />

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_survey_desc_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/item_dc_duty_patient_disease_diagnosis_tv"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="16dp"
                    android:layout_toRightOf="@+id/item_dc_duty_patient_disease_survey_tv"
                    android:layout_weight="1"
                    android:gravity="center|left"
                    android:singleLine="true"
                    android:text="慢性阻塞性肺疾病反反复复发烧佛哦吼粉红佛 off 放开那就是对方 i 哦肌肤防腐剂哦素"
                    android:textColor="@color/color_1a1a1a"
                    android:textSize="@dimen/font_size_14"
                    android:visibility="visible" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/item_dc_duty_patient_disease_check_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical">

                <View
                    android:id="@+id/item_dc_duty_patient_disease_check_v"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_below="@+id/item_dc_duty_patient_disease_survey_tv"
                    android:layout_gravity="center"
                    android:background="@drawable/bg_solid_666666_patient_point" />

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_check_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/item_dc_duty_patient_disease_survey_tv"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/item_dc_duty_patient_disease_check_v"
                    android:gravity="center|left"
                    android:text="检查检验:"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/font_size_14" />

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_check_desc_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/item_dc_duty_patient_disease_survey_tv"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="16dp"
                    android:layout_toRightOf="@+id/item_dc_duty_patient_disease_check_tv"
                    android:layout_weight="1"
                    android:gravity="center|left"
                    android:singleLine="true"
                    android:text="慢性阻塞性肺疾病反反复复发烧佛哦吼粉红佛 off 放开那就是对方 i 哦肌肤防腐剂哦素"
                    android:textColor="@color/color_1a1a1a"
                    android:textSize="@dimen/font_size_14"
                    android:visibility="visible" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/item_dc_duty_patient_disease_prescription_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical">

                <View
                    android:id="@+id/item_dc_duty_patient_disease_prescription_v"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_below="@+id/item_dc_duty_patient_disease_check_tv"
                    android:layout_gravity="center"
                    android:background="@drawable/bg_solid_666666_patient_point" />

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_prescription_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/item_dc_duty_patient_disease_check_tv"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/item_dc_duty_patient_disease_prescription_v"
                    android:gravity="center|left"
                    android:text="用药及处方:"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/font_size_14" />

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_prescription_desc_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/item_dc_duty_patient_disease_check_tv"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="16dp"
                    android:layout_toRightOf="@+id/item_dc_duty_patient_disease_prescription_tv"
                    android:layout_weight="1"
                    android:gravity="center|left"
                    android:singleLine="true"
                    android:text="慢性阻塞性肺疾病反反复复发烧佛哦吼粉红佛 off 放开那就是对方 i 哦肌肤防腐剂哦素"
                    android:textColor="@color/color_1a1a1a"
                    android:textSize="@dimen/font_size_14"
                    android:visibility="visible" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/item_dc_duty_patient_disease_file_photo_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:visibility="visible">

                <TextView
                    android:id="@+id/item_dc_duty_patient_disease_data_and_pic_tv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:text="图片"
                    android:textColor="@color/color_1a1a1a"
                    android:textSize="@dimen/font_size_14"
                    android:visibility="visible"/>
            </LinearLayout>

        </LinearLayout>

    </RelativeLayout>

</LinearLayout>